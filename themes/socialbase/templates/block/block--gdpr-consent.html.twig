{% extends 'block.html.twig' %}

{% block content %}
    {{ attach_library('socialbase/block--informblock') }}
    <div class="card__block">
        {{ content|without('link') }}
    </div>

    <footer class="card__actionbar">
        {{ content.link }}
    </footer>
{% endblock %}
